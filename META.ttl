		rdfs:label "FreeMind::Document";
		nfo:fileName "lib/FreeMind/Document.pm";
		nfo:programmingLanguage "Perl";
	];
	cpant:requires    "Type::Utils 0.000_08"^^cpant:dsWithVersion;
	cpant:requires    "Types::Standard 0.000_08"^^cpant:dsWithVersion;
	cpant:requires    "XML::LibXML::Augment 0.001"^^cpant:dsWithVersion;
	cpant:test_requires "Test::LongString 0.15"^^cpant:dsWithVersion;
	cpant:version_from [
		a nfo:FileDataObject;
		a nfo:SourceCode;
		rdfs:label "FreeMind::Document";
