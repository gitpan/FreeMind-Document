	doap:bug-database <http://rt.cpan.org/Dist/Display.html?Queue=FreeMind-Document>;
	doap:created      "2013-04-06"^^xsd:date;
	doap:developer    cpan:tobyink;
	doap:download-page <https://metacpan.org/release/FreeMind-Document>;
	doap:homepage     <https://metacpan.org/release/FreeMind-Document>;
	doap:license      <http://dev.perl.org/licenses/>;
