	doap:programming-language "Perl";
	doap:release      dist:v_0-000_01;
	doap:repository   [
		a doap:HgRepository;
		doap:browse <https://bitbucket.org/tobyink/p5-freemind-document>;
	];
	doap:shortdesc    "representation of a FreeMind-style mind map document".

dist:v_0-000_01
