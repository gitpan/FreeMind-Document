		a nfo:FileDataObject;
		a nfo:SourceCode;
		rdfs:label "FreeMind::Document";
		nfo:fileName "lib/FreeMind/Document.pm";
		nfo:programmingLanguage "Perl";
	];
	cpant:readme_from [
		a nfo:FileDataObject;
